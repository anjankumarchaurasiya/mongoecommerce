<div class="page-header">
  <h3 class="page-title"> Brand </h3>

</div>
<div class="row">
  <!-- DataTales Example -->
  <div class="col-md-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body tree">
        <% if (data.length) { %>
        <ul class="category-tree-list">
          <% } %>

          <% data.forEach(function(brand){ %>
          <li>
            <div class="icon-name-group">

              <%= brand.name %>
              <span class="icon-group"><span class="btn mdi text-secondary mdi-playlist-edit"></span><a href="/category/delete/<%- brand._id %>" class="href-link"><span class="btn mdi mdi-delete text-secondary"></span></a></span>
            </div>
          </li>
          <% }) %>

          <% if (data.length) { %>
        </ul>
        <% }else{ %>
        <p class="text-center">Data not available</p>
        <% } %>
      </div>
    </div>
  </div>
</div>