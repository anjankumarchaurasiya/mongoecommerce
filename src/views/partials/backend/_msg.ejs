<% if(success_msg.length>0){ %>
<div class="alert alert-success">
  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
  <%=success_msg%>
</div>
<% } %>
<% if(success.length>0){ %>
<div class="alert alert-success">
  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
  <%=success%>
</div>
<% } %>
<% if(error_msg.length>0){ %>
<div class="alert alert-error">
  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
  <%=error_msg%>
</div>
<% } %>
<% if(error.length>0){ %>
<div class="alert alert-success">
  <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
  <%=error%>
</div>
<% } %>